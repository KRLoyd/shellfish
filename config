SLEEPSECONDS=1
LTRACE=`which ltrace`
LTRACEOUTPUTFILE="checker_ltrace_output_$R"
ECHO=`which echo`
CAT=`which cat`
GREP=`which grep`
WC=`which wc`
RM=`which rm`
TOUCH=`which touch`
CHMOD=`which chmod`
COMPILE="gcc -Wall -Werror -Wextra -pedantic *.c -o hsh"
